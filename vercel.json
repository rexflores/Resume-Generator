{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://github.com/rexflores/Resume-Generator-no_img"
    }
  ]
  }
  